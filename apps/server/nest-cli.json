{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "main",
  "compilerOptions": {
    "deleteOutDir": true,
    "watchAssets": true,
    "webpack": true,
    "webpackConfigPath": "webpack.config.js"
  },
  "watchOptions": {
    "watchPolling": true,
    "pollInterval": 1000
  }
}
